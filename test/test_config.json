[
  {
    "configName": "image",
    "basePathName": "imageBasePath",
    "basePath": "lib/resources/images",
    "className": "ImageNames",
    "manageType": [
      "png",
      "json"
    ],
    "meta": {
      "className": "ImageMeta",
      "params": [
        {
          "name": "pattern",
          "type": "regex"
        },
        {
          "name": "citeArray",
          "type": "array"
        }
      ]
    },
    "strategy": {
      "needCopy": false,
      "needOverride": true,
      "removeNoCiteResource": true,
      "removeNoCiteStr": true
    },
    "imageStrategy": {
      "ratio": 3,
      "lastPattern": "@${num}x"
    }
  }
]

